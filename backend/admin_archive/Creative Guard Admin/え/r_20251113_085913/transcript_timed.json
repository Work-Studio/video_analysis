{
  "segments": [
    {
      "timecode_start": "00:01.0",
      "timecode_end": "00:02.4",
      "text": "お誕生日おめでとう！"
    },
    {
      "timecode_start": "00:04.1",
      "timecode_end": "00:04.6",
      "text": "めでたくない。"
    },
    {
      "timecode_start": "00:05.0",
      "timecode_end": "00:05.5",
      "text": "めでたくない。"
    },
    {
      "timecode_start": "00:06.2",
      "timecode_end": "00:06.6",
      "text": "かほ。"
    },
    {
      "timecode_start": "00:07.1",
      "timecode_end": "00:07.4",
      "text": "言っちゃって。"
    },
    {
      "timecode_start": "00:08.4",
      "timecode_end": "00:08.7",
      "text": "ぺー。"
    },
    {
      "timecode_start": "00:10.0",
      "timecode_end": "00:12.1",
      "text": "今日からあんたは女の子じゃない。"
    },
    {
      "timecode_start": "00:14.9",
      "timecode_end": "00:15.1",
      "text": "はあ。"
    },
    {
      "timecode_start": "00:15.2",
      "timecode_end": "00:15.7",
      "text": "25歳波面だよ。"
    },
    {
      "timecode_start": "00:16.5",
      "timecode_end": "00:18.1",
      "text": "この中一気にリアルワールド。"
    },
    {
      "timecode_start": "00:18.2",
      "timecode_end": "00:18.4",
      "text": "もう。"
    },
    {
      "timecode_start": "00:18.4",
      "timecode_end": "00:20.7",
      "text": "チヤホヤされないし褒めてもくれない。"
    },
    {
      "timecode_start": "00:20.7",
      "timecode_end": "00:25.1",
      "text": "下にはキラキラした後輩、週末ごとにアップされる結婚式の写真。"
    },
    {
      "timecode_start": "00:25.1",
      "timecode_end": "00:25.4",
      "text": "白い。"
    },
    {
      "timecode_start": "00:25.4",
      "timecode_end": "00:25.8",
      "text": "タキシード受ける。"
    },
    {
      "timecode_start": "00:25.8",
      "timecode_end": "00:26.0",
      "text": "元彼。"
    },
    {
      "timecode_start": "00:26.0",
      "timecode_end": "00:26.6",
      "text": "このまじやばい。"
    },
    {
      "timecode_start": "00:26.6",
      "timecode_end": "00:27.1",
      "text": "なんで？"
    },
    {
      "timecode_start": "00:27.1",
      "timecode_end": "00:27.7",
      "text": "いつからこうなった？"
    },
    {
      "timecode_start": "00:27.7",
      "timecode_end": "00:31.1",
      "text": "可愛いという武器はもはやこの手にはない。"
    },
    {
      "timecode_start": "00:31.7",
      "timecode_end": "00:33.0",
      "text": "でも可愛い大人だっているもん。"
    },
    {
      "timecode_start": "00:33.2",
      "timecode_end": "00:33.7",
      "text": "いるよねえ。"
    },
    {
      "timecode_start": "00:34.0",
      "timecode_end": "00:34.3",
      "text": "どっちよ。"
    },
    {
      "timecode_start": "00:35.2",
      "timecode_end": "00:37.2",
      "text": "可愛いをアップデートできる女になるか。"
    },
    {
      "timecode_start": "00:37.4",
      "timecode_end": "00:38.6",
      "text": "このままステイか。"
    },
    {
      "timecode_start": "00:39.2",
      "timecode_end": "00:39.9",
      "text": "ってこと？"
    },
    {
      "timecode_start": "00:40.5",
      "timecode_end": "00:40.8",
      "text": "かも。"
    },
    {
      "timecode_start": "00:41.4",
      "timecode_end": "00:41.8",
      "text": "私こっち。"
    },
    {
      "timecode_start": "00:42.1",
      "timecode_end": "00:42.7",
      "text": "私も私も。"
    },
    {
      "timecode_start": "00:45.4",
      "timecode_end": "00:48.6",
      "text": "なんか燃えてきた。"
    }
  ]
}